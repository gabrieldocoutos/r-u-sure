{
  "manifest_version": 3,
  "name": "Hello Extensions",
  "description": "Base Level Extension",
  "version": "1.0",
  "action": {
    "default_popup": "hello.html"
  },
  "permissions": [       "storage",
  "activeTab",
  "scripting",
  "tabs"],
  "content_scripts": [
    {
        "all_frames": true,
        "matches": [
            "<all_urls>"
        ],
        "js": [
            "content.js"
        ]
    }
]
}
